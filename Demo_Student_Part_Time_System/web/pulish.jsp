               <option value="陕西">陕西</option>
               <option value="江西">江西</option>
               <option value="江苏">江苏</option>
               <option value="浙江">浙江</option>
               <option value="福建">福建</option>
               <option value="上海">上海</option>
               <option value="黑龙江">黑龙江</option>
               <option value="吉林">吉林</option>
               <option value="辽宁">辽宁</option>
               <option value="海南">海南</option>
               <option value="香港">香港</option>
               <option value="澳门">澳门</option>
               <option value="台湾">台湾</option>
               </select>
         </div>
         <div class="txt1">  
               学历要求：<select id="select" class="txtphone">
               <option value="时">高中生在读</option>
               <option value="日">本科在读</option>
               <option value="周">研究生在读</option>
               </select>
        </div> 
        <div class="txt1">     
                工资结算： <select id="select" class="txtphone">
               <option value="时">时</option>
               <option value="日">日</option>
               <option value="周">周</option>
               <option value="月">月</option>
               <option value="随意">随意</option>
               </select>
        </div> 
        <div class="txt1"><input type="submit" id="input" value="提交"class="tijiao"></div>
        </div>
        <div class="right2">
        <div class="txt1">
               工资数目：<input name="" type="text"class="txtphone"/>
        </div> 
        <div class="txt1">
               工作时间：<input name="" type="text"class="txtphone"/>
         </div> 
        <div class="txt1">   
               联系人&nbsp：<input name="" type="text"class="txtphone"/> 
        </div> 
        <div class="txt1">
               联系电话：<input name="" type="text"class="txtphone"/>
        </div> 
        
        <div class="txt1">
